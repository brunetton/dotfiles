[
    {
        "name": "Open path in clipboard with Nemo",
        "icon": null,
        "script": "nemo \"`xclip -selection c -o`\" > /dev/null"
    },
    {
        "name": "Copy process command",
        "description": "Click onto process to copy command",
        "icon": null,
        "script": "id=`xprop _NET_WM_PID | awk '/PID/ {print $3}'`; cat /proc/$id/cmdline | xargs -0 | xclip -selection clipboard"
    },
    {
        "name": "Commit Zim notes",
        "icon": null,
        "script": "xterm -hold -e \". ~/.ssh/agent.env > /dev/null; cd ~/zim-notes; git push; echo '\nEND'\" "
    },
    {
        "name": "Commit passwords",
        "description": "Commit + push",
        "icon": null,
        "script": "xterm -hold -e \". ~/.ssh/agent.env > /dev/null; cd ~/.passwords; git add -u --ignore-removal &> /dev/null; git ci --untracked-files=no -m 'update'; git push; echo '\nEND'\""
    },
    {
        "name": "Mount naq (sshfs)",
        "icon": null,
        "script": "cd sshfs; ./mount_naq.sh"
    },
    {
        "name": "Mount tel -> ~/Android",
        "icon": null,
        "script": "fusermount -u ~/Android; go-mtpfs ~/Android"
    },
    {
        "name": "Mount Google drive (~/Google drive)",
        "icon": null,
        "script": "fusermount -u '/home/bruno/Google drive'; sleep 1; google-drive-ocamlfuse '/home/bruno/Google drive/' &"
    },
    {
        "name": "Unmount Google drive kill (~/Google drive)",
        "icon": null,
        "script": "fusermount -u '/home/bruno/Google drive'; killall google-drive-ocamlfuse -9"
    },
    {
        "name": "Stop transferts sshfs foireux",
        "icon": null,
        "script": "killall kioslave5 -9"
    },
    {
        "name": "Resolve sync Notes conflits",
        "icon": null,
        "script": "/home/bruno/dev/dotfiles/scripts/open_notes_conflicts.py"
    },
    {
        "name": "Kill pause process",
        "icon": null,
        "script": "kill -s STOP $(xprop _NET_WM_PID | awk '/PID/ {print $3}')"
    },
    {
        "name": "Kill start process",
        "icon": null,
        "script": "kill -s CONT $(xprop _NET_WM_PID | awk '/PID/ {print $3}')"
    },
    {
        "name": "Paste to QRcode",
        "icon": null,
        "script": "xterm -title CustomScript -hold -e \"qrencode -m 2 -t utf8 <<< '$(xclip -selection c -o)'\""
    },
    {
        "name": "Copy last downloaded file path as filepath",
        "icon": null,
        "script": "echo \"file://$(/bin/ls -t -d ~/Téléchargements/* | head -1)\" | xclip -sel clip -t text/uri-list"
    },
    {
        "name": "Delete last downloaded file",
        "description": "With confirmation",
        "icon": null,
        "script": "file=$(/bin/ls -t -d ~/Téléchargements/* | head -1); zenity --question --text \"Delete $file ?\n\n(enter = YES)\" --width=450 --ok-label='_Yes' --cancel-label='_No' && rm -f \"$file\""
    },
    {
        "name": "Copy last downloaded file path as text",
        "icon": null,
        "script": "/bin/ls -t -d ~/Téléchargements/* | head -1 | xclip -r -selection clipboard"
    },
    {
        "name": "clipboard -> basename -> clipboard",
        "icon": null,
        "script": "basename \"$(xclip -selection c -o)\" | xclip -r -selection clipboard"
    },
    {
        "name": "Play in VLC",
        "icon": null,
        "script": "vlc \"$(xclip -selection c -o)\""
    },
    {
        "name": "test",
        "icon": null,
        "script": "echo $(ssh-agent) | xclip -r -selection clipboard"
    }
]