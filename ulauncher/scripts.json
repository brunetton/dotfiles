[
    {
        "name": "Open copied path with Nemo",
        "icon": null,
        "script": "nemo \"`xclip -selection c -o`\" > /dev/null"
    },
    {
        "name": "Copy process command",
        "description": "Click onto process to copy command",
        "icon": null,
        "script": "id=`xprop _NET_WM_PID | awk '/PID/ {print $3}'`; cat /proc/$id/cmdline | xargs -0 | xclip -selection clipboard"
    },
    {
        "name": "Commit Zim notes",
        "icon": null,
        "script": "xterm -hold -e \"cd ~/zim-notes; git push\""
    },
    {
        "name": "Mount naq (sshfs)",
        "icon": null,
        "script": "cd sshfs; ./mount_naq.sh"
    },
    {
        "name": "Mount Google drive (~/drive)",
        "icon": null,
        "script": "rclone mount 'Google Drive:' '/home/bruno/Google drive' &"
    }
]