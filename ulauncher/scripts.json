[
    {
        "name": "Open copied path with Nemo",
        "icon": null,
        "script": "nemo \"`xclip -selection c -o`\" > /dev/null"
    },
    {
        "name": "Copy process command",
        "description": "Click onto process to copy command",
        "icon": null,
        "script": "id=`xprop _NET_WM_PID | awk '/PID/ {print $3}'`; cat /proc/$id/cmdline | xargs -0 | xclip -selection clipboard"
    },
    {
        "name": "Commit Zim notes",
        "icon": null,
        "script": "xterm -hold -e \". ~/.ssh/agent.env > /dev/null; cd ~/zim-notes; git push\""
    },
    {
        "name": "Commit passwords",
        "description": "Commit + push",
        "icon": null,
        "script": "xterm -hold -e \". ~/.ssh/agent.env > /dev/null; cd ~/.passwords; git add -u --ignore-removal &> /dev/null; git ci -m 'update'; git push; echo '\nEND'\""
    },
