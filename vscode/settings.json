{
    "editor.fontSize": 13,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        ".vscode": true,
        ".ipynb*": true,
        "**/*.{pyc,pyo}": { "when": "$(basename).py" }
    },
    "terminal.integrated.fontSize": 11,
    "python.formatting.autopep8Args": [
        "--max-line-length=120"
    ],
    "python.linting.pylintArgs": [
